{
  "manifest_version": 2,

  "name": "Pivy-print",
  "description": "This extension enables print for Pivotal tracker.",
  "version": "0.8.0",

  "permissions": [
    "https://www.pivotaltracker.com/",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.pivotaltracker.com/*"],
      "js": ["jquery.min.js", "pivy_print.js"],
      "css": ["pivy_print.css"]
    }
  ]
}